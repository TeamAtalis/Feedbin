<h1><%= @user.email %></h1> 

<% @my_profiles.each do |profile| %>
  <h1><%= profile.profile_name %> --- <%= profile.profile_name %> --- <%= profile.profile_name %></h1>
  <% profile.tags.each do |tag| %>
    <li><%= tag.name %></li>
  <% end %>
<% end %>
<br/>
<h1>Add profiles</h1>
<br/>
<% @profiles.each do |profile| %>
  <h1><%= link_to profile.profile_name, subscribe_path(profile_id: profile.id), method: :get, class: 'btn btn-primary' %>

<% end %>